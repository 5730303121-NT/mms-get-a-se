<div class="container my-5">
    <div class="row d-flex justify-content-center">
        <h4 class="">About {{search.results_count}} results</h4>
    </div>

    {{#each search.results}}
    <div class="row">
        {{#each this}}
        <div class="result col-sm-12 col-md-4 col-lg-4 my-2">
            <a id="result-card" href="/service/{{this.id}}">
                <div class="card" style="width: 100%; height:100%">
                    <img class="card-img-top" src="/images/index_images/bg1_old.jpg" alt="Card image cap">
                    <div class="card-body">
                        <h4 class="card-title">
                            <i class="fa fa-circle {{this.status}}" aria-hidden="true"></i> {{this.name}}</h4>
                        <p class="card-text">{{this.about}}</p>
                        <span class="price-tag">{{this.price}} Baht/hr</span>
                        <span class="rate-tag">
                            {{#times this.fullstar}}
                                <i class="fa fa-star mx-0 px-0" aria-hidden="true"></i>
                            {{/times}}
                            {{#times this.halfstar}}
                                <i class="fa fa-star-half-o  mx-0 px-0" aria-hidden="true"></i>
                            {{/times}}
                            {{#times this.emptystar}}
                                <i class="fa fa-star-o mx-0 px-0" aria-hidden="true"></i>
                            {{/times}}
                        </span>
                    </div>
                </div>
            </a>
        </div>
        {{/each}}
    </div>
    {{/each}}
</div>